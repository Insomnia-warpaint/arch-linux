{
    "workbench.colorTheme": "Atom One Dark",
    "workbench.sideBar.location": "right",
    "files.autoSave": "afterDelay",
    "editor.fontSize": 16,
    "editor.fontFamily": "'FantasqueSansMono Nerd Font Mono','Source Han Mono Sc'",
    "editor.wordWrap": "on",
    "editor.cursorBlinking": "expand",
    "editor.cursorWidth": 6,
    "editor.cursorSmoothCaretAnimation": true,
    "powermode.enabled": true,
    "powermode.presets": "flames",
    "git.enableSmartCommit": true,
    "git.autofetch": true,
    "editor.suggestSelection": "first",
    "workbench.iconTheme": "vscode-great-icons",
    "C_Cpp.updateChannel": "Insiders",
    "code-runner.runInTerminal": true,
    "bracketPairColorizer.depreciation-notice": false,
    "files.exclude": {
        "**/.classpath": false,
        "**/.project": true,
        "**/.settings": true,
        "**/.factorypath": true
    },
    "java.import.gradle.java.home": "/usr/lib/jvm/java-11-openjdk/",
    "java.import.gradle.version": "7.2",
    "java.import.gradle.wrapper.enabled": false,
    "java.import.gradle.home": "/usr/share/java/gradle/",
    "explorer.compactFolders": false,
    "powermode.shake.enabled": false

}